{
  "manifest_version": 2,
  "name": "Memrise Scraper",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.memrise.com/course/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "browser_action": {}
}
